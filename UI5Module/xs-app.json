{
  "welcomeFile": "/UI5Module/test/flpSandbox.html",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/Northwind/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "Northwind",
      "csrfProtection": false
    },
    {
      "source": "^/UI5Module/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}